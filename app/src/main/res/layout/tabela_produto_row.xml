                    android:layout_alignParentEnd="true"
                    android:text			= "1400,00"
                    android:textColor		= "#0000CC"
                    android:gravity			= "right"/>
            </RelativeLayout>



            <RelativeLayout
                android:id = "@+id/ll_faixa_04"
                android:layout_height="wrap_content"
                android:layout_width= "match_parent"
                android:layout_weight	="1"
                android:background	= "@drawable/fundo_neutro">
                <TextView
                    android:id				= "@+id/lbl_faixa_04"
                    android:layout_height   ="wrap_content"
                    android:layout_width    ="wrap_content"
                    android:text			= "Faixa 04"
                    android:gravity			= "center"
                    android:textSize        ="12sp"
                    android:textColor="@color/red"/>

                <TextView
                    android:id				= "@+id/txt_faixa_04"
                    android:layout_height   = "wrap_content"
                    android:layout_width    = "wrap_content"
                    android:layout_below    = "@+id/lbl_faixa_04"
                    android:layout_alignParentEnd="true"
                    android:text			= "1400,00"
                    android:textColor		= "#0000CC"
                    android:gravity			= "right"/>
            </RelativeLayout>

            <RelativeLayout
                android:id = "@+id/ll_faixa_05"
                android:layout_height="wrap_content"
                android:layout_width= "match_parent"
                android:layout_weight	="1"
                android:background	= "@drawable/fundo_neutro">
                <TextView
                    android:id				= "@+id/lbl_faixa_05"
                    android:layout_height   ="wrap_content"
                    android:layout_width    ="wrap_content"
                    android:text			= "Faixa 05"
                    android:gravity			= "center"
                    android:textSize        ="12sp"
                    android:textColor="@color/red"/>

                <TextView
                    android:id				= "@+id/txt_faixa_05"
                    android:layout_height   = "wrap_content"
                    android:layout_width    = "wrap_content"
                    android:layout_below    = "@+id/lbl_faixa_05"
