<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:background="@drawable/fundo"
	>


	<LinearLayout
		android:id="@+id/parentLayout"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:layout_alignParentTop="true"
		android:layout_alignParentStart="true">

		<TableLayout
			android:id="@+id/table_497"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:shrinkColumns="*"
			android:stretchColumns="*"
			>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="match_parent">


				<TextView
					android:id="@+id/txt_nro_pedido_497"
					android:layout_width="fill_parent"
					android:layout_height="25dp"
					android:gravity="left|center_vertical"
					android:text="Nº 07-999999"
					android:textColor="@color/dark_blue"
					android:textSize="14sp"

					/>


				<TextView
					android:id="@+id/txt_nro_tablete_497"
					android:layout_width="fill_parent"
					android:layout_height="25dp"
					android:gravity="left|center_vertical"
					android:text="Situacao: Faturado"
					android:textColor="@color/dark_blue"
					android:textSize="14sp"

					/>


			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="match_parent">


				<TextView
					android:id="@+id/txt_emissao_497"
					android:layout_width="fill_parent"
					android:layout_height="25dp"
					android:gravity="left|center_vertical"
					android:text="Nº 07-999999"
					android:textColor="@color/dark_blue"
					android:textSize="14sp"

					/>


				<TextView
					android:id="@+id/txt_entrega_497"
					android:layout_width="fill_parent"
					android:layout_height="25dp"
					android:gravity="left|center_vertical"
					android:text="Situacao: Faturado"
					android:textColor="@color/dark_blue"
					android:textSize="14sp"

					/>


			</TableRow>


			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_cliente_497"
					android:layout_span="2"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="000000-00 CARREFOUR CAMPINAS LTDA"
					android:textColor="@color/black"
					/>
			</TableRow>


			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_cnpj_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="CNPJ/CPF: 62.901.210.0001-08"
					android:textColor="@color/black"
					android:textSize="14sp"
					/>

				<TextView
					android:id="@+id/txt_ie_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="I.E.: 244.244.255.244"
					android:textColor="@color/black"
					android:textSize="14sp"
					/>

			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_cidade_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="CIDADE:CAMPINAS"
					android:textColor="@color/black"
					/>

				<TextView
					android:id="@+id/txt_telefone_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="TELEFONE (19) 8383838383"
					android:textColor="@color/black"
					/>

			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_condpagto_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="Cond.Pagto: 28 dll"
					android:textColor="@color/black"
					/>

				<TextView
					android:id="@+id/txt_tabpreco_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="Tab. Preço: Tabela Garantida 02"
					android:textColor="@color/black"
					/>

			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_cliente_entrega_497"
					android:layout_span="2"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="000000-00 CLIENTE ENTREGA"
					android:textColor="@color/black"
					/>
			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_cidade_entrega_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="CIDADE:CAMPINAS"
					android:textColor="@color/black"
					/>

				<TextView
					android:id="@+id/txt_telefone_entrega_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="TELEFONE (19) 8383838383"
					android:textColor="@color/black"
					/>

			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >
				<TextView
					android:id="@+id/txt_obs_pedido_497"
					android:layout_span="2"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="000000-00 CARREFOUR CAMPINAS LTDA"
					android:textColor="@color/black"
					/>
			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >
				<TextView
					android:id="@+id/txt_obs_nf_497"
					android:layout_span="2"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="000000-00 CARREFOUR CAMPINAS LTDA"
					android:textColor="@color/black"
					/>
			</TableRow>

			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >
				<TextView
					android:id="@+id/txt_frete_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="frete: CIF/FOB "
					android:textColor="@color/black"
					/>
				<TextView
					android:id="@+id/txt_frete_desc_retira_497"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:text="Desconto Retira"
					android:textColor="@color/black"
					/>

			</TableRow>


			<TableRow
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" >

				<TextView
					android:id="@+id/txt_total_497"
					android:layout_span="2"
					android:layout_width="fill_parent"
					android:layout_height="35dp"
					android:gravity="left|center_vertical"
					android:text="Total Do Pedido: 340,00"
					android:textColor="@color/orange"
					android:textSize="14sp"
					/>
			</TableRow>




		</TableLayout>

		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/fundo_preto"
			android:orientation="horizontal" >

		</LinearLayout>
	</LinearLayout>

</RelativeLayout>
