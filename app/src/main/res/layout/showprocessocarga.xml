<?xml version="1.0" encoding="utf-8"?>
   <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:paddingBottom="@dimen/activity_vertical_margin"
		    android:paddingLeft="@dimen/activity_horizontal_margin"
		    android:paddingRight="@dimen/activity_horizontal_margin"
		    android:paddingTop="@dimen/activity_vertical_margin"
			android:background="@drawable/customcategoria">


	   <TextView
		   android:id="@+id/txt_titulo_127"
		   android:layout_width="fill_parent"
		   android:layout_height="wrap_content"
		   android:layout_alignParentTop="true"
		   android:gravity="center"
		   android:text="Processo Em Andamento"
		   android:textSize="24dp" />


	<TextView
		android:id="@+id/lbl_estagio_127"
		android:layout_width="150dp"
		android:layout_height="wrap_content"
		android:layout_below="@+id/txt_titulo_127"
		android:layout_marginLeft="5dp"
		android:layout_marginTop="10dp"
		android:text="Estágio"
		android:textSize="16dp"/>

	<TextView
		android:id="@+id/txt_estagio_127"
		android:layout_width="match_parent"
		android:layout_height="30dp"
		android:layout_marginRight="100dp"
		android:layout_below="@+id/lbl_estagio_127"
		android:background="@drawable/fundo"
		android:gravity="center"
		android:textSize="16dp"/>




	<TextView
		android:id="@+id/lbl_obs_127"
		android:layout_below="@+id/txt_estagio_127"
		android:layout_marginTop="10dp"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="5dp"
		android:text="Observação"
		android:textSize="16dp"
		/>

	<TextView
		android:id="@+id/txt_obs_127"
		android:layout_below="@+id/lbl_obs_127"
		android:layout_width="fill_parent"
		android:layout_height="30sp"
		android:background="@drawable/fundo"
		android:textSize="16dp"/>

        <Button
   	           android:id="@+id/bt_cancela_127"
   	           android:layout_below="@+id/txt_obs_127"
   	           android:layout_marginTop="15dp"
   	           android:layout_alignParentRight="true"
   	           android:layout_width="180dp"
   	           android:layout_height="wrap_content"
   	           android:background="@drawable/fundo"
   	           android:fontFamily="Helvetica"
			   android:text="Cancela"
               />
        
        
       
   </RelativeLayout>

