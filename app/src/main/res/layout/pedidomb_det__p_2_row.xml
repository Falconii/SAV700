<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginLeft="5sp"
    android:layout_marginRight="5sp"
    android:layout_marginTop="10sp"
    android:layout_marginBottom="10sp"
    android:background="@drawable/fundopedido"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_produto_006"
            android:layout_width="0sp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
            android:layout_weight="0.7"
            android:text="PRODUTO"
            android:textColor="@color/red"
            android:textSize="12sp"
            android:background="@drawable/titleproduto"
            />


        <TextView
            android:id="@+id/txt_status_006"
            android:layout_width="0sp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
            android:layout_weight="0.3"
            android:text="PRODUTO"
            android:textColor="@color/red"
            android:textSize="12sp"
            android:background="@drawable/titleproduto"

            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_mensagem_006"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
            android:text=""
            android:textColor="@color/red"
            android:textSize="12sp"
            android:background="@drawable/detalheproduto"
            />

    </LinearLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="05dp"
        android:layout_marginBottom="05dp"
        >

        <TableLayout
            android:id="@+id/table_006"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:shrinkColumns="*"
            android:stretchColumns="*"
            >


            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/lbl_operacao_006"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="OPERAÇÃO"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />


                <TextView
                    android:id="@+id/lbl_qtd_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="QTD"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />


                <TextView
                    android:id="@+id/lbl_prcven_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="P.UNIT"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />

                <TextView
                    android:id="@+id/lbl_desconto_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="DESC %"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />


                <TextView
                    android:id="@+id/lbl_desconto_verba_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Desc. Verba %"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />

                <TextView
                    android:id="@+id/lbl_acordo_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="ACORDO"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />

                <TextView
                    android:id="@+id/lbl_total_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="TOTAL"
                    android:textColor="@color/red"
                    android:textSize="12sp"
                    android:background="@drawable/titleproduto"

                    />

            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/txt_venda_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center"
                    android:text="VENDA"
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />


                <TextView
                    android:id="@+id/txt_qtd_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />


                <TextView
                    android:id="@+id/txt_prcven_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

                <TextView
                    android:id="@+id/txt_desconto_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

                <TextView
                    android:id="@+id/txt_desconto_verba_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"

                    />

                <TextView
                    android:id="@+id/txt_acordo_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"

                    />

                <TextView
                    android:id="@+id/txt_total_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/txt_bonificacao_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center"
                    android:text="BONIF."
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />



                <TextView
                    android:id="@+id/txt_qtd_boni_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />


                <TextView
                    android:id="@+id/txt_prcven_boni_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

                <TextView
                    android:id="@+id/txt_desconto_boni_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

                <TextView
                    android:id="@+id/txt_desconto_verba_boni_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"

                    />

                <TextView
                    android:id="@+id/txt_acordo_boni_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    android:text=" " />
                <TextView
                    android:id="@+id/txt_total_boni_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >
                <TextView
                    android:id="@+id/txt_descricao_verba_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center"
                    android:layout_span="04"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />
                <TextView
                    android:id="@+id/txt_descricao_acordo_006"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center"
                    android:layout_span="03"
                    android:text=" "
                    android:textColor="@color/dark_blue"
                    android:textSize="12sp"
                    android:background="@drawable/detalheproduto"
                    />

            </TableRow>

        </TableLayout>

    </RelativeLayout>

</LinearLayout>