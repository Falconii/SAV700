        android:text="Base Do RelatÃ³rio"
        android:textColor="@color/red"
        android:background="@drawable/fundo"
        >
    </TextView>


    <LinearLayout
        android:id="@+id/ll_visao_726"
        android:layout_width="match_parent"
        android:layout_below="@+id/lbl_titulo_726"
